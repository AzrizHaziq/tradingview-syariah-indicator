{
  "type": "module",
  "name": "@app/data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "ava",
    "for-china-etf": "use npm pdfreader",
    "update-data": "node scripts/_index.mjs",
    "update-data:skip-commit": "npm run update-data -- skip-commit"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "cli-progress": "^3.8.2",
    "msw": "^0.35.0",
    "playwright-chromium": "^1.11.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "ava": {
    "files": [
      "__test__/**/*.mjs"
    ],
    "concurrency": 2,
    "failFast": true,
    "failWithoutAssertions": false,
    "environmentVariables": {
      "MY_ENVIRONMENT_VARIABLE": "some value"
    },
    "verbose": true,
    "nodeArguments": [
      "--trace-deprecation",
      "--napi-modules"
    ]
  }
}
