{
  "name": "@app/extension",
  "version": "2.0.3",
  "homepage": "https://tsi.azrizhaziq.com",
  "description": "Display small shariah indicator to stock list in Tradingview",
  "bugs": {
    "url": "https://github.com/AzrizHaziq/tradingview-syariah-indicator/issues",
    "email": "azrizhaziq@gmail.com"
  },
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch",
    "watch:ff": "web-ext run",
    "watch:c": "npm run watch:ff -- --target=chromium",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:BA": "ENABLE_BA=true npm run build",
    "postbuild": "web-ext lint && ts-node scripts/ff-source-code.ts",
    "prerelease": "npm run build && web-ext build -s dist -o -n extension.zip",
    "lint": "eslint --fix .",
    "test": "playwright test --config=playwright.config.ts"
  },
  "license": "MIT",
